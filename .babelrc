{
    "presets": ["@babel/preset-react", "@babel/preset-env"],
    "plugins": [
        [
            "module-resolver",
            {
                "alias": {
                    "assets": "./src/assets",
                    "components": "./src/components",
                    "containers": "./src/containers",
                    "core": "./src/core",
                    "contexts": "./src/contexts",
                    "features": "./src/features",
                    "hooks": "./src/hooks",
                    "providers": "./src/providers",
                    "themes": "./src/themes",
                    "utils": "./src/utils"
                }
            }
        ],
        [
            "react-intl",
            {
                "messagesDir": "./public/"
            }
        ]
    ]
}
